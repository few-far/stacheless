{
    "name": "few-far/stacheless",
    "autoload": {
        "psr-4": {
            "FewFar\\Stacheless\\": "src",
            "Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Stacheless",
            "description": "Few and Far CMS addon"
        },
        "laravel": {
            "providers": [
                "FewFar\\Stacheless\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "statamic/cms": "4.32.0",
        "orchestra/testbench": "^8",
        "pestphp/pest": "^2.24",
        "pestphp/pest-plugin-laravel": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    }
}
